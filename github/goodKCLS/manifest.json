{
	"manifest_version": 2,
	"name": "goodKCLS-local",
	"version": "1.2.2",
	"description": "Launch King County Library Catalog searches from Goodreads.com",
	"icons": { "128": "books-icon-128.png" },
	"page_action": {
		"default_name": "Search KCLS",
		"default_icon": {"32": "books-icon-32.png"}
		},
	"background": {
		"scripts": ["gotback.js"],
		"persistent": false
		},
	"permissions": [
		"tabs",
		"http://catalog.kcls.org/",
		"http://www.goodreads.com/"
	]
}