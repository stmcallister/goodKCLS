{
	"manifest_version": 2,
	"name": "goodKCLS-local",
	"version": "1.2.13",
	"description": "Launch King County Library Catalog searches from Goodreads.com",
	"icons": { "128": "books-icon-128.png" },
	"page_action": {
		"default_name": "Search KCLS",
		"default_icon": "books-icon-32.png"
		},
	"background": {
		"scripts": ["gotback.js"],
		"persistent": false
		},
	"permissions": [
		"activeTab",
		"tabs",
		"http://catalog.kcls.org/",
		"https://www.goodreads.com/"
	],
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}